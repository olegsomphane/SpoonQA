from python/python
using gnu/wget

cmd mkdir C:\test
workdir c:\test

cmd wget https://github.com/olegsomphane/SpoonQA/raw/master/python/resources/hello.py -O hello.py --no-check-certificate
cmd wget https://github.com/olegsomphane/SpoonQA/raw/master/python/resources/hello.expected.txt -O hello.expected.txt --no-check-certificate

cmd wget https://github.com/olegsomphane/SpoonQA/raw/master/sqa-utils/compare/compare.exe -O compare.exe --no-check-certificate

cmd python.exe c:\test\hello.py > hello.result.txt

cmd compare.exe "C:\test\hello.expected.txt" "C:\test\hello.result.txt"